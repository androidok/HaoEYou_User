<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context="com.haoeyou.user.activity.FillingActivity">

    <include
        android:id="@+id/toolbar_layout"
        layout="@layout/toolbar_layout"
        />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/icon_person_info"
                    android:drawablePadding="10dp"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="基本资料"
                    android:textColor="#3eb0ff"
                    android:textSize="13sp"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#d6e2f5"/>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="姓名"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@null"
                    android:gravity="center"
                    android:hint="请填写真实姓名"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="性别"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/sex"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请选性别"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="出生年月"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/birthday"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请选择出生年月"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="年龄"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/age"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="12dp"
                android:background="#fbfcfe"/>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="联系电话"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/tel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请输入电话"
                    android:inputType="phone"
                    android:maxLength="15"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="紧急联系人电话"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/urgent_tel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请输入电话"
                    android:inputType="phone"
                    android:maxLength="15"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="所在城市"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/area"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请选择所在地址"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="12dp"
                android:background="#fbfcfe"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#d6e2f5"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/icon_care_departmen"
                    android:drawablePadding="10dp"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="关注科室"
                    android:textColor="#3eb0ff"
                    android:textSize="13sp"/>
            </LinearLayout>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="vertical"
                android:paddingLeft="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:text="关注科室(选填)"
                    android:textColor="#000000"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/department"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:hint="请选择馆关注科室"
                    android:paddingBottom="12dp"
                    android:paddingRight="10dp"
                    android:paddingTop="16dp"
                    android:textColor="#000000"
                    android:textColorHint="#b9c9e1"
                    android:textSize="16sp"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/icon_more"/>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginBottom="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#d6e2f5"/>

            <include
                layout="@layout/layout_button"
                android:visibility="gone"
                />
        </LinearLayout>
    </ScrollView>
</LinearLayout>
