package com.haoeyou.user.adapter;

import android.view.View;
import android.widget.LinearLayout;
import android.widget.TextView;

import com.haoeyou.user.R;
import com.haoeyou.user.activity.ReportActivity;
import com.haoeyou.user.fragment.FourthFragment;

import butterknife.Bind;
import butterknife.OnClick;


/**
 * Created by Administrator on 2017/3/7.
 */
public class NameListHolder extends BaseHolder<String> {

    @Bind(R.id.name)
    TextView mName;

    public NameListHolder(View view) {
        super(view);
    }

    @Override
    public void setData(String mData) {
        super.setData(mData);
        mName.setText(mData);
    }

    @Override
    public void init() {
        super.init();
        mView.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                listener.onItemClick(v, getPosition(), mData);
            }
        });
    }
}
